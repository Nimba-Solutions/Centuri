<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ISBLANK_CANCELATION_DATE</fullName>
    <active>true</active>
    <description>30491</description>
    <errorConditionFormula>AND(
ISBLANK(   Cancelation_Date__c   ),
TEXT( Status__c )=&apos;Canceled&apos;)</errorConditionFormula>
    <errorMessage>Project Cancelation Date is required when Status is Canceled</errorMessage>
</ValidationRule>
