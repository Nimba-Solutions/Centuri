<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QUANTITY_NON_NEGATIVE</fullName>
    <active>true</active>
    <description>Users cannot enter a negative quantity.</description>
    <errorConditionFormula>Quantity__c &lt; 0</errorConditionFormula>
    <errorMessage>Quantity cannot be negative.</errorMessage>
</ValidationRule>
