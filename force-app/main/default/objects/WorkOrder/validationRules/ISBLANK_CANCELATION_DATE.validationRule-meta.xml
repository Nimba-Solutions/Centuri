<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ISBLANK_CANCELATION_DATE</fullName>
    <active>true</active>
    <description>#29507</description>
    <errorConditionFormula>AND(
ISBLANK(  Cancelation_Date__c  ),
TEXT(  Status  ) = &apos;Canceled&apos;)</errorConditionFormula>
    <errorMessage>Work Order Cancelation Date is required when status is Canceled</errorMessage>
</ValidationRule>
