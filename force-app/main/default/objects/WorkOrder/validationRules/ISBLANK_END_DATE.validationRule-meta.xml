<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ISBLANK_END_DATE</fullName>
    <active>true</active>
    <description>#29507</description>
    <errorConditionFormula>AND(
ISBLANK( EndDate),
TEXT(  Status  ) = &apos;Completed&apos;
)</errorConditionFormula>
    <errorMessage>Work Order End Date is required when status is Completed or Canceled</errorMessage>
</ValidationRule>
