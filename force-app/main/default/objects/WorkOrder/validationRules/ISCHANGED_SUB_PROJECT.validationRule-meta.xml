<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ISCHANGED_SUB_PROJECT</fullName>
    <active>true</active>
    <description>#28654 Sub Project should not change.</description>
    <errorConditionFormula>AND( $Profile.Name &lt;&gt;&apos;System Administrator&apos;,PRIORVALUE(Sub_Project__c)&lt;&gt;&apos;&apos;,
ISCHANGED( Sub_Project__c ))</errorConditionFormula>
    <errorMessage>Work Order Sub Project should not change. You may cancel and recreate a work order as needed or contact a System Adminstrator.</errorMessage>
</ValidationRule>
