<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LockoutDate__c</fullName>
    <description>The date represents the lock out date from LockTime object(This field was created for NPL MA).</description>
    <formula>DATEVALUE(
				IF(!ISBLANK(ParentTimesheet__r.LockoutDateTime__c ),
													ParentTimesheet__r.LockoutDateTime__c, 
													ParentTimesheet__r.Default_LockoutDate_Time__c )
)</formula>
    <label>LockoutDate</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
