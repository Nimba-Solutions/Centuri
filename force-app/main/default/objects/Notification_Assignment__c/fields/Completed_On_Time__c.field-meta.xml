<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Completed_On_Time__c</fullName>
    <description>BA: NPLMA - Determines if mobile user completed assignment before due date</description>
    <formula>IF(Date_Completed__c  &gt;  Mobile_Notification__r.Due_Date__c, TRUE, FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Completed On Time?</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
