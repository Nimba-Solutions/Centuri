<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Posting_Key__c</fullName>
    <description>Add WBS or Center Center calculation if WBS element is blank(This field was created for NPL MA).</description>
    <externalId>false</externalId>
    <formula>IF( !ISBLANK(WBSElement__c ), 
			WBSElement__r.SAP_WBS_ElementWithCodingMask__c, 
			CostCenter__r.Name)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Posting Key</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
