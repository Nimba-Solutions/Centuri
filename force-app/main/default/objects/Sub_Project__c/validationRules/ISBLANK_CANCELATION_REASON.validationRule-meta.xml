<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ISBLANK_CANCELATION_REASON</fullName>
    <active>true</active>
    <description>29526</description>
    <errorConditionFormula>AND(
ISBLANK(  Cancelation_Reason__c   ),
TEXT( Status__c ) = &apos;Canceled&apos;)</errorConditionFormula>
    <errorMessage>Sub Project Cancelation Reason is required when status is Canceled</errorMessage>
</ValidationRule>
